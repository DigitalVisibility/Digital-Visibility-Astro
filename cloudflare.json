{
  "speed": {
    "rocket_loader": true,
    "early_hints": true,
    "smart_hints": true,
    "image_resizing": true,
    "speed_brain": true,
    "cloudflare_fonts": true,
    "content_optimization": {
      "auto_minify": {
        "html": true,
        "css": true, 
        "js": true
      },
      "brotli": true,
      "http2": true,
      "http3": true
    }
  },
  "caching": {
    "browser_ttl": 3600,
    "cache_level": "aggressive",
    "edge_ttl": 86400,
    "development_mode": false,
    "always_online": true,
    "serve_stale": true
  },
  "image_optimization": {
    "polish": "lossless",
    "webp": true,
    "formats": ["webp", "avif"],
    "quality": "auto"
  }
}
